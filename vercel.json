{
"version": 2,
"builds": [
  {
    "src": "api/wsgi.py",
    "use": "@vercel/python"
    
  },
  {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "staticfiles"
    }
  }
],
"routes": [
  {
    "src": "/(.*)",
    "dest": "api/wsgi.py"
  }
],
"outputDirectory": "staticfiles"
}